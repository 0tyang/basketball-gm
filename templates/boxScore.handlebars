<h2><a href="/l/{{lid}}/roster/{{game.won.abbrev}}/{{game.season}}">{{game.won.region}} {{game.won.name}}</a> {{game.won.pts}}, <a href="/l/{{lid}}/roster/{{game.lost.abbrev}}/{{game.season}}">{{game.lost.region}} {{game.lost.name}}</a> {{game.lost.pts}}{{overtime}}</h2>
{{#game.teams}}
  <h3><a href="/l/{{../lid}}/roster/{{abbrev}}/{{../game.season}}">{{region}} {{name}}</a></h2>
  <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered table-condensed">
  <thead>
    <tr><th>Name</th><th>Pos</th><th>Min</th><th>FG</th><th>3Pt</th><th>FT</th><th>Off</th><th>Reb</th><th>Ast</th><th>TO</th><th>Stl</th><th>Blk</th><th>PF</th><th>Pts</th></tr>
  </thead>
  <tbody>
  {{#players}}
    <tr{{#if separator}} class="separator"{{/if}}><td><a href="/l/{{../../lid}}/player/{{pid}}">{{name}}</a>{{skillsBlock skills}}</td><td>{{pos}}</td><td>{{round min 1}}</td><td>{{fg}}-{{fga}}</td><td>{{tp}}-{{tpa}}</td><td>{{ft}}-{{fta}}</td><td>{{orb}}</td><td>{{trb}}</td><td>{{ast}}</td><td>{{tov}}</td><td>{{stl}}</td><td>{{blk}}</td><td>{{pf}}</td><td>{{pts}}</td></tr>
  {{/players}}
  </tbody>
  <tfoot>
    <tr><td>Total</td><td></td><td>{{min}}</td><td>{{fg}}-{{fga}}</td><td>{{tp}}-{{tpa}}</td><td>{{ft}}-{{fta}}</td><td>{{orb}}</td><td>{{trb}}</td><td>{{ast}}</td><td>{{tov}}</td><td>{{stl}}</td><td>{{blk}}</td><td>{{pf}}</td><td>{{pts}}</td></tr>
  </tfoot>
  </table>
{{/game.teams}}
